{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": { "url": "data/cars.csv" },
  "mark": "bar",
  "encoding": {
    "column": {"field": "body-style", "type": "nominal", "title": "Body Style"},
    "x": {"field": "city-mpg", "type": "quantitative", "bin": true, "title": "City MPG"},
    "y": {"field": "*", "type": "quantitative", "aggregate": "count"}
  }
}