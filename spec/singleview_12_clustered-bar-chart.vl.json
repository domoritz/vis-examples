{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": { "url": "data/cars.csv" },
  "mark": "bar",
  "encoding": {
    "row": {"field": "body-style", "type": "nominal", "header": {"title": "Body Style"}},
    "y": {"field": "fuel", "type": "nominal", "axis": {"title": "Fuel Type"}},
    "x": {"aggregate": "average", "field": "city-mpg", "type": "quantitative", "axis": {"title": "City MPG"}}
  }
}