{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {
    "url": "data/census-income-pivoted.csv",
    "format": {"type": "csv", "parse": {"order": "number", "total": "number"}}
  },
  "mark": "bar",
  "encoding": {
    "column": {
      "field": "key",
      "type": "ordinal",
      "sort": {
        "op": "min",
        "field": "order"
      }
    },
    "x": {
      "field": "value",
      "type": "quantitative"
    },
    "y": {
      "field": "Geography",
      "type": "nominal",
      "sort": {
        "op": "min",
        "field": "total"
      }
    }
  }
}
