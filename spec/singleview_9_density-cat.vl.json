{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "config": {"background":"#fdf5e6"},    
  "data": { "url": "data/cars.csv" },
  "mark": "rect",
  "encoding": {
    "y": {"field": "make", "type": "nominal", "axis": {"title": "Make"}},
    "x": {"field": "body-style", "type": "nominal", "axis": {"title": "Body Style"}},
    "color": {"aggregate": "count", "field": "*", "type": "quantitative"}
  },
  "config": {
    "range": {
      "heatmap": {
        "scheme": "greenblue"
      }
    }
  }
}