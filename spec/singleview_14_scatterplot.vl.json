{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": { "url": "data/cars.csv" },
  "mark": "point",
  "encoding": {
    "color": {"field": "body-style", "type": "nominal", "legend": {"title": "Body Style"}},
    "y": {"field": "curb-weight", "type": "quantitative", "axis": {"title": "Curb Weight"}},
    "x": {"field": "city-mpg", "type": "quantitative", "axis": {"title": "City MPG"}}
  }
}